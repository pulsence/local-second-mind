# Optional path to a custom config file used by integration/live tests
LSM_TEST_CONFIG=

# Live LLM provider credentials
LSM_TEST_OPENAI_API_KEY=
LSM_TEST_ANTHROPIC_API_KEY=
LSM_TEST_GOOGLE_API_KEY=
LSM_TEST_OLLAMA_BASE_URL=http://localhost:11434

# Live remote provider credentials
LSM_TEST_BRAVE_API_KEY=
LSM_TEST_SEMANTIC_SCHOLAR_API_KEY=
LSM_TEST_CORE_API_KEY=

# Live PostgreSQL + pgvector integration
# Example: postgresql://user:password@localhost:5432/lsm_test
# The role should have CREATE on schema `public` and pgvector available.
LSM_TEST_POSTGRES_CONNECTION_STRING=

# Embedding and tier defaults
LSM_TEST_EMBED_MODEL=sentence-transformers/all-MiniLM-L6-v2
LSM_TEST_TIER=smoke
